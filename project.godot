config_version=5

[application]

config/name="Light-Hearted Story"
run/main_scene="res://src/scenes/gardens.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SwapPlayers="*res://src/scripts/SwapPlayers.gd"
DataManager="*res://src/scripts/DataManager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=768
window/size/viewport_height=432
window/size/window_width_override=1440
window/size/window_height_override=768
window/stretch/mode="canvas_items"
window/fallback="it"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/tile_bit_tools/plugin.cfg")

